{
    "name": "dandadin/webserver",
    "description": "This is a web server backend for dandadin's web",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dandadin/deca.git"
        }
    ],
    "require": {
        "dandadin/deca-sites": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "trap 'mv ./vendor/dandadin/deca-sites/*.php /webserver/html/deca/' EXIT",
            "trap 'mv ./vendor/dandadin/deca-sites/*.html /webserver/html/deca/' EXIT"
        ]
    }
}
    